{
	"name": "gracenode",
	"description": "A node.js framework for real-time applications over UDP and TCP protocols with extendable modules for quick and clean evelopment.",
	"version": "3.5.13",
	"author": "Nobuyori Takahashi <voltrue2@yahoo.com>",
	"repository": {
		"type": "git",
		"url": "https://github.com/voltrue2/gracenode.git"
	},
	"scripts": {
		"test": "make lint && make test",
		"preinstall": "rm -f ../../gracenode",
		"install": "ln -fs node_modules/gracenode/aeterno ../../gracenode"
	},
	"dependencies": {
		"jshint": "2.9.3",
		"gracelog": "0.6.9",
		"aeterno": "0.6.5",
		"cluster-mode": "1.2.0",
		"node-datetime": "0.1.5",
		"multiparty": "4.1.2",
		"cookies": "0.5.1"
	},
	"devDependencies": {
		"mocha": "2.3.3",
		"staticdata": "0.0.7",
		"gracenode-server": "0.4.3",
		"request": "2.75.0",
		"express": "4.13.3"
	},
	"engine": {
		"engine": "node >= 4.0.0"
	},
	"jshintConfig": {
                "esversion": 6,
		"node": true,
                "bitwise": false,
                "camelcase": true,
                "curly": true,
                "eqeqeq": true,
                "forin": false,
                "immed": true,
                "latedef": false,
                "newcap": true,
                "noarg": true,
                "noempty": true,
                "undef": true,
                "unused": true,
                "nonew": true,
                "white": true,
                "maxdepth": 5,
                "quotmark": "single",
                "globals": {
                        "mocha": false,
                        "describe": false,
                        "it": false,
                        "before": false,
                        "beforeEach": false,
                        "after": false,
                        "afterEach": false
                }
        },
	"keywords": [
		"framework",
		"grace",
		"node",
		"gracenode",
		"boostrap",
		"cluster",
		"daemon",
		"express",
		"light",
		"organize",
		"structure",
		"modular",
		"router",
		"udp",
		"realtime",
		"real-time",
		"real time",
		"session",
		"web",
		"api",
		"http",
		"udp",
		"rpc",
		"tcp",
		"render",
		"template",
		"rest",
		"game"
	],
	"license": "MIT"
}
